<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyVersion>2.0.0.12</AssemblyVersion>
    <FileVersion>2.0.0.12</FileVersion>
    <Version>2.0.0</Version>
    <AssemblyName>Bitai.LDAPHelper.DTO</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Viko Bastidas (BITAI)</Authors>
    <Company>BITAI</Company>
    <Product>LDAP Services Wrappers</Product>
    <Description>DTO classes to generate output data by Bitai.LDAPHelper and to be used in other applications.</Description>
    <Copyright></Copyright>
    <PackageIcon>hierarchy_32.png</PackageIcon>
    <RootNamespace>Bitai.LDAPHelper.DTO</RootNamespace>
    <PackageId>Bitai.LDAPHelper.DTO</PackageId>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/bitai-cs/LDAPHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bitai-cs/LDAPHelper.git</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>ldap, authentication, directory, helper</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\resources\hierarchy_32.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
